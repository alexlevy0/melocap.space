		"start:web": "expo start -c",
		"start:sandbox": "yarn ampx sandbox --stream-function-logs",
		"start:dev": "expo start --dev-client",
		"start-native": "expo start -c",
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"native:ios": "npx expo run",
		"web": "expo start --web",
		"build:web": "npx expo export -p web",
		"test": "jest",
		"generate-themes": "tamagui generate-themes ./theme-builder.ts ./theme-output.ts",
		"format": "biome check --no-errors-on-unmatched --apply .",
		"build:liveness:docker": "cd liveness-ts-image && docker build --platform linux/amd64 -t docker-image:test .",
		"test:liveness:docker": "cd liveness-ts-image && docker run --platform linux/amd64 -p 9000:8080 docker-image:test",
		"deploying:liveness:docker:login": "cd liveness-ts-image && aws ecr get-login-password --region us-west-2 | docker login --username AWS --password-stdin 327852390890.dkr.ecr.us-west-2.amazonaws.com",
		"deploying:liveness:docker:create:repository": "cd liveness-ts-image && aws ecr create-repository --repository-name liveness-ts --region us-west-2 --image-scanning-configuration scanOnPush=true --image-tag-mutability MUTABLE",
		"deploying:liveness:docker:create:tag": "ts-node liveness-ts-image/scripts/dockerManager.ts tag",
		"deploying:liveness:docker:push": "ts-node liveness-ts-image/scripts/dockerManager.ts push"
	},
